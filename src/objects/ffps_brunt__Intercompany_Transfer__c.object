    <validationRules>
        <fullName>ffps_brunt__Cannot_change_from_complete</fullName>
        <active>true</active>
        <description>Cannot change status from complete</description>
        <errorConditionFormula>AND(
ISPICKVAL(PRIORVALUE(ffps_brunt__Processing_Status__c), &apos;Complete&apos;),
ISCHANGED(ffps_brunt__Processing_Status__c))</errorConditionFormula>
        <errorMessage>You cannot change the status of the ICT once it is &apos;Complete&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ffps_brunt__Cannot_move_to_complete</fullName>
        <active>true</active>
        <description>Cannot move an incomplete ICT to &apos;Complete&apos;</description>
        <errorConditionFormula>AND( 
ISPICKVAL(ffps_brunt__Processing_Status__c, &apos;Complete&apos;), 
ISCHANGED(ffps_brunt__Processing_Status__c),
OR(ISBLANK(ffps_brunt__Destination_Journal_Number__c ),
   ISBLANK(ffps_brunt__Source_Journal_Number__c ))
)</errorConditionFormula>
        <errorMessage>Cannot move an incomplete ICT to &apos;Complete&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ffps_brunt__Destination_bank_account_company</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ffps_brunt__Destination_Bank_Account__c != NULL, ffps_brunt__Destination_Company__c != ffps_brunt__Destination_Bank_Account__r.c2g__OwnerCompany__c)</errorConditionFormula>
        <errorDisplayField>ffps_brunt__Destination_Bank_Account__c</errorDisplayField>
        <errorMessage>Destination bank account must match the destination company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ffps_brunt__Source_Bank_account_company</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ffps_brunt__Source_Bank_Account__c != NULL,
 ffps_brunt__Source_Bank_Account__r.c2g__OwnerCompany__c !=  ffps_brunt__Source_Company__c)</errorConditionFormula>
        <errorDisplayField>ffps_brunt__Source_Bank_Account__c</errorDisplayField>
        <errorMessage>Source bank account company must match source company</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ffps_brunt__Source_and_destination_companies</fullName>
        <active>true</active>
        <description>Source and destination companies cannot be the same</description>
        <errorConditionFormula>AND(ffps_brunt__Intra_Company_Transfer__c != true, ffps_brunt__Source_Company__c ==  ffps_brunt__Destination_Company__c)</errorConditionFormula>
        <errorMessage>Source and destination companies cannot be the same.</errorMessage>
    </validationRules>
