    <validationRules>
        <fullName>Check_For_Contact_Email_Defined</fullName>
        <active>true</active>
        <description>A user cannot create Indicative Cost without Contact Email (Every time when Indicative Cost created, we send it to the customer).</description>
        <errorConditionFormula>ISBLANK(Contact__r.Email)</errorConditionFormula>
        <errorDisplayField>Contact__c</errorDisplayField>
        <errorMessage>Cannot send to Contact - No Email defined!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Concession_Percent_Between_0_and_100</fullName>
        <active>true</active>
        <errorConditionFormula>OR (
 Concession_Percent__c &lt; 0,
 Concession_Percent__c &gt; 1
)</errorConditionFormula>
        <errorDisplayField>Concession_Percent__c</errorDisplayField>
        <errorMessage>Concession % must be a number from 0 to 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Density_And_Specification_Blank_Error</fullName>
        <active>true</active>
        <description>Specification cannot be blank if Density is not blank</description>
        <errorConditionFormula>NOT(ISPICKVAL(Density__c, &apos;&apos;)) &amp;&amp; ISPICKVAL(Specification__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Specification__c</errorDisplayField>
        <errorMessage>Specification cannot be blank if Density is not blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Density_And_Specification_Not_Blank_Err</fullName>
        <active>true</active>
        <description>Density cannot be blank if Specification is not blank</description>
        <errorConditionFormula>ISPICKVAL(Density__c, &apos;&apos;) &amp;&amp; NOT(ISPICKVAL(Specification__c, &apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Density__c</errorDisplayField>
        <errorMessage>Density cannot be blank if Specification is not blank</errorMessage>
    </validationRules>
