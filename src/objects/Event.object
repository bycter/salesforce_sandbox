    <validationRules>
        <fullName>Agent_Broker_Validation</fullName>
        <active>true</active>
        <description>Required if Viewing Arranged by = Agent or Broker</description>
        <errorConditionFormula>AND(
    TEXT(Viewing_Arranged_by__c) = &quot;Agent_Broker&quot;,
    ISBLANK(Agent_Broker__c),
    RecordType.DeveloperName = &apos;Viewing&apos;
   )</errorConditionFormula>
        <errorDisplayField>Agent_Broker__c</errorDisplayField>
        <errorMessage>Complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ChangeArrangedByInThePast</fullName>
        <active>true</active>
        <description>Arranged by can&apos;t be changed for Viewings in the past</description>
        <errorConditionFormula>AND(
    RecordType.DeveloperName = &apos;Viewing&apos;, 
    ActivityDateTime &lt; NOW(),
    ISCHANGED(Viewing_Arranged_by__c)
)</errorConditionFormula>
        <errorMessage>Arranged by can&apos;t be changed for Viewings in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Disable_Feedback</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISNEW(),
RecordType.DeveloperName = &apos;Viewing&apos;,
ActivityDateTime &lt; NOW(),
Dont_send_feedback__c = FALSE,
NOT(ISPICKVAL( Viewing_Conducted_by__c , &apos;Agent_Broker&apos;) ) 
)</errorConditionFormula>
        <errorDisplayField>Dont_send_feedback__c</errorDisplayField>
        <errorMessage>If a viewing is in the past, feedback should be disabled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reminder_Cannot_Be_After_Event_Date</fullName>
        <active>true</active>
        <errorConditionFormula>DATEVALUE(Reminder__c) &gt;= DATEVALUE(ActivityDateTime)</errorConditionFormula>
        <errorDisplayField>Reminder__c</errorDisplayField>
        <errorMessage>The reminder can&apos;t be after or on the viewing date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reminder_Earliest_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
ISNEW(), 
ISCHANGED( Reminder__c ) 
), 
RecordType.DeveloperName = &apos;Viewing&apos;, 
NOT(ISBLANK( Reminder__c )), 
Disable_Reminder__c = FALSE,
DATEVALUE(Reminder__c) &lt;= TODAY()
)</errorConditionFormula>
        <errorDisplayField>Reminder__c</errorDisplayField>
        <errorMessage>Please set an earliest reminder date of tomorrow or later.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reminder_Not_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED( Reminder__c )
),
RecordType.DeveloperName = &apos;Viewing&apos;,
NOT(ISBLANK( Reminder__c )),
Disable_Reminder__c = FALSE,
OR(
DATEVALUE(ActivityDateTime) &lt;= TODAY(),
DATEVALUE(ActivityDateTime) &lt; TODAY()+1
)
)</errorConditionFormula>
        <errorDisplayField>Reminder__c</errorDisplayField>
        <errorMessage>No reminder is required for this viewing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Lead_or_Opportunity_on_Viewing</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Viewing&apos;, OR(NOT(BEGINS(WhoId, &apos;00Q&apos;)), ISBLANK(WhoId)), OR(NOT(BEGINS(WhatId, &apos;006&apos;)), ISBLANK(WhatId)))</errorConditionFormula>
        <errorMessage>Select Lead or Opportunity on Viewing!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Viewing_Conducted_By_User_Validation</fullName>
        <active>true</active>
        <description>Required if Viewing Conducted by not Agent or Broker</description>
        <errorConditionFormula>AND(
            TEXT(Viewing_Conducted_by__c) &lt;&gt; &quot;Agent_Broker&quot;,
            ISBLANK(Viewing_Conducted_by_User__c),
            RecordType.DeveloperName = &apos;Viewing&apos;
            )</errorConditionFormula>
        <errorDisplayField>Viewing_Conducted_by_User__c</errorDisplayField>
        <errorMessage>Complete this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Viewing_Must_Be_Marked_As_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
            ISNEW(),
            RecordType.DeveloperName = &apos;Viewing&apos;,
            ActivityDateTime &lt; NOW(),
            NOT( ISPICKVAL( Status__c , &apos;Completed&apos;) )
            )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>If a viewing is in the past, the status must be &apos;Completed&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Viewing_Rescheduled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  RecordType.DeveloperName = &apos;Viewing&apos;,
            Rescheduled__c = FALSE,
OR(
ISCHANGED( ActivityDateTime ),
ISCHANGED( Property__c ),
AND(
ISCHANGED( Status__c ),
ISPICKVAL( Status__c , &apos;Booked&apos;),
OR(
ISPICKVAL( PRIORVALUE( Status__c ), &apos;Confirmed&apos;),
ISPICKVAL( PRIORVALUE( Status__c ), &apos;Cancelled&apos;),
ISPICKVAL( PRIORVALUE( Status__c ), &apos;Did Not Attend&apos;)
)
)
)
)</errorConditionFormula>
        <errorDisplayField>Rescheduled__c</errorDisplayField>
        <errorMessage>You have amended the viewing, so please indicate that it has been rescheduled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wait_To_Rescedule_Again</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED( Rescheduled__c ) ,
Rescheduled__c = FALSE,
PRIORVALUE(Rescheduled__c ) = TRUE )</errorConditionFormula>
        <errorDisplayField>Rescheduled__c</errorDisplayField>
        <errorMessage>Please wait a few minutes before rescheduling again.</errorMessage>
    </validationRules>
</CustomObject>
