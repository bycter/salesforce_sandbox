        <fullName>Enforce_Acc_Dep_GLA_With_Setting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
                Not($Setup.FAM__Fixed_Asset_Management_Settings__c.Ignore_FAM_FFA_validation_rules__c),

                AND( IsBlank(Accumulated_Depreciation_GL_Account__c), 
                    AND(IsBlank( FAM__Asset_Group__r.Accumulated_Depreciation_GL_Account__c),
                        ( IsBlank(FAM__Asset_Sub_Group__r.Accumulated_Depreciation_GL_Account__c))
                    )
                )
