    <fields>
        <fullName>FAM__Special_Depreciation_Allowance_Percent__c</fullName>
        <deprecated>false</deprecated>
        <description>The percentage calculated for the special depreciation allowance.</description>
        <externalId>false</externalId>
        <formula>IF ( FAM__Claim_Special_Depreciation_Allowance__c = TRUE, 

IF( FAM__Fixed_Asset__r.FAM__Date_In_Service__c &gt;= DATE(2015,1,1) &amp;&amp; FAM__Fixed_Asset__r.FAM__Date_In_Service__c &lt;= DATE(2017,12,31), 0.5, 

IF( FAM__Fixed_Asset__r.FAM__Date_In_Service__c &gt;= DATE(2018,1,1) &amp;&amp; FAM__Fixed_Asset__r.FAM__Date_In_Service__c &lt;= DATE(2018,12,31), 0.4, 

IF( FAM__Fixed_Asset__r.FAM__Date_In_Service__c &gt;= DATE(2019,1,1) &amp;&amp; FAM__Fixed_Asset__r.FAM__Date_In_Service__c &lt;= DATE(2019,12,31), 0.3, 

0))),

0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Based on the asset&apos;s year in service, this is the percentage used to calculate the special depreciation allowance.</inlineHelpText>
        <label>Special Depreciation Allowance Percent</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
