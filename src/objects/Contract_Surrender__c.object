    <validationRules>
        <fullName>BillingStatusesRequiredForCompletion</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Complete&apos;),
 OR(
    ISPICKVAL(Break_Penalty_Premium_Billing_Status__c,&apos;&apos;),
    ISPICKVAL(Deposit_Billing_Status__c,&apos;&apos;),
    ISPICKVAL(Deposit_Interest_Billing_Status__c,&apos;&apos;),
    ISPICKVAL(Suite_Restoration_Billing_Status__c,&apos;&apos;),
    ISPICKVAL(Terminal_Dilapidations_Billing_Status__c,&apos;&apos;),
    ISPICKVAL(Final_Dilapidations_Billing_Status__c,&apos;&apos;),
    AND(RecordType.DeveloperName=&apos;Serviced&apos;, ISPICKVAL(Surrender_Premium_Billing_Status__c,&apos;&apos;)),
    AND(NOT(RecordType.DeveloperName=&apos;Serviced&apos;),ISPICKVAL(Interim_Dilapidations_Billing_Status__c,&apos;&apos;)),
    AND(NOT(RecordType.DeveloperName=&apos;Serviced&apos;),ISPICKVAL(Budget_Dilapidations_Billing_Status__c,&apos;&apos;))
 )
)</errorConditionFormula>
        <errorMessage>Populate all Billing Status fields for saving with Status &apos;Completed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Currency_cannot_be_negative</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
    AND(NOT(ISBLANK(Break_Penalty_Premium__c)), Break_Penalty_Premium__c &lt; 0),
    AND(NOT(ISBLANK(Deposit__c)), Deposit__c &lt; 0),
    AND(NOT(ISBLANK(Deposit_Interest__c)), Deposit_Interest__c &lt; 0),
    AND(NOT(ISBLANK(Final_Dilapidations__c)), Final_Dilapidations__c &lt; 0),
    AND(NOT(ISBLANK(Suite_Restoration__c)), Suite_Restoration__c &lt; 0),
    AND(NOT(ISBLANK(Terminal_Dilapidations__c)), Terminal_Dilapidations__c &lt; 0),
    AND(NOT(ISBLANK(Surrender_Premium__c)), Surrender_Premium__c &lt; 0),
    AND(NOT(ISBLANK(Interim_Dilapidations__c)), Interim_Dilapidations__c &lt; 0),
    AND(NOT(ISBLANK(Budget_Dilapidations__c)), Budget_Dilapidations__c &lt; 0)
)</errorConditionFormula>
        <errorMessage>Values can&apos;t be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dilapidations_not_negative</fullName>
        <active>true</active>
        <errorConditionFormula>Final_Dilapidations__c &lt; 0</errorConditionFormula>
        <errorMessage>Dilapidations can&apos;t be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Surrender_Liabilities_not_negative</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Surrender_Premium__c &lt; 0,
  RecordType.DeveloperName == &apos;Serviced&apos;
)</errorConditionFormula>
        <errorMessage>Surrender Liabilities can&apos;t be negative</errorMessage>
    </validationRules>
