    <validationRules>
        <fullName>Check_Related_To_For_FileTypeTask</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;File_type_task&apos;
&amp;&amp;
ISBLANK( WhatId )</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>File Type Task should be related to an Account, Opportunity or Case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CostPlan_Budget_Value_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &apos;Cost_Plan&apos;,
ISBLANK(Budget_Value__c),
NOT(ISBLANK(TEXT(Status_Complete__c)))
)</errorConditionFormula>
        <errorDisplayField>Budget_Value__c</errorDisplayField>
        <errorMessage>Budget Value field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CostPlan_StatusTaskComleted_required</fullName>
        <active>true</active>
        <description>if Cost_Plan task, Status Complete field required</description>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &apos;Cost_Plan&apos;,
  ISPICKVAL(Status, &apos;Completed&apos;),
  ISBLANK(TEXT(Status_Complete__c))
)</errorConditionFormula>
        <errorMessage>Status Complete field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Instruction_FieldsValidation_required</fullName>
        <active>true</active>
        <description>if Instruction task, Practical Completion Date (Actual) field is required</description>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &apos;Instruction&apos;,
  ISPICKVAL(Status, &apos;Completed&apos;),
  ISBLANK( Completion_Date_Actual__c )
)</errorConditionFormula>
        <errorDisplayField>Completion_Date_Actual__c</errorDisplayField>
        <errorMessage>Field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Link_task_to_Case</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(RecordType.DeveloperName = &apos;Design&apos;, RecordType.DeveloperName = &apos;Cost_Plan&apos;, RecordType.DeveloperName = &apos;Instruction&apos;, RecordType.DeveloperName = &apos;Quotation&apos;),
ISBLANK(WhatId)
)</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>Please relate the task to a record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quotation_FieldsValidation_required</fullName>
        <active>true</active>
        <description>if Quotation task,  Concept Reference, Quotation Amount and Indicative Programme Length fields are required</description>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &apos;Quotation&apos;,
  ISPICKVAL(Status, &apos;Completed&apos;),
  OR(
    ISBLANK( Concept_Reference__c ),
    ISBLANK( Quotation_Amount__c ),
    ISBLANK( Indicative_Programme_Length__c )
  )
)</errorConditionFormula>
        <errorMessage>Concept Reference, Quotation Amount and Indicative Programme Length fields are required.</errorMessage>
    </validationRules>
</CustomObject>
