<apex:page renderAs="pdf" showHeader="false" sidebar="false" standardStylesheets="false" applyBodyTag="false"
           applyHtmlTag="false" cache="false" controller="ServicedSpaceLicenceAgreementGenerator">

    <html>
    <head>
        <apex:stylesheet value="{!$Resource.ServicedQuote}"/>
        <style>
            .agreement_text_standard {
                font-family: arial;
                font-size: 10pt;
            }

            .agreement_text_large {
                font-family: arial;
                font-size: 10pt;
                border: 1px solid black;
            }

            li1 span {
                padding-left: 12px;
            }

            li1.subitem {
                padding-left: 40px;
            }

            .disguised {
                color: white;
            }

            .hide {
                color: white;
            }

            div.signature table td, th {
                width: 200px;
                text-align: left;
                padding-top: 4px;
                padding-bottom: 4px;
                min-height: 20px;
            }

            div.signature table {
                margin-bottom: 40px;
            }

            .contactDescription {
                margin-bottom: 10px;
            }

            .contactType {
                font-size: 10.5pt;
                font-weight: bold;
            }

            .contactHelpText {
                font-size: 9pt;
