                <td>{!mapProducts['BSLF'][0].Suite_Inventory__r.Name}</td>
            </tr>
        </apex:outputPanel>

        <apex:outputPanel layout="none" rendered="{!CONTAINS(codes, 'BSLF,')}">
            <tr>
                <td><b>Number of Workstations: </b></td>
