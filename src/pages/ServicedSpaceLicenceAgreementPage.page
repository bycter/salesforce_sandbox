                            <apex:variable var="endPostfix" value="{!IF(AND(steppedRent.Rent_Step_To__c> 10, steppedRent.Rent_Step_To__c < 14),"th", CASE(MOD(steppedRent.Rent_Step_To__c,10),1,"st",2,"nd",3,"rd","th"))}"/>
