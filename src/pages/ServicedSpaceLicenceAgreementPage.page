                    <b>
                        <apex:outputText value="{0, number, £###,##0.00}">
                            <apex:param value="{!IF(CONTAINS(codes, 'BSAS,'), mapProducts['BSLF'][0].New_Monthly_Average__c + mapProducts['BSAS'][0].New_Monthly_Average__c, mapProducts['BSLF'][0].New_Monthly_Average__c)}"/>
                        </apex:outputText>
